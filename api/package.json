{
  "name": "@sushiii/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:unit": "vitest run --reporter=verbose src/**/*.test.ts",
    "test:integration": "vitest run src/__tests__/integration",
    "test:e2e": "vitest run src/__tests__/e2e",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:seed": "tsx scripts/setup-database.ts",
    "setup-db": "tsx scripts/setup-database.ts",
    "generate-wallet": "tsx scripts/generate-dag4-wallet.ts"
  },
  "dependencies": {
    "@stardust-collective/dag4": "^2.1.1",
    "@noble/ed25519": "^2.0.0",
    "@noble/curves": "^1.3.0",
    "@prisma/client": "^5.8.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "zod": "^3.22.4",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "prom-client": "^15.1.0",
    "ioredis": "^5.3.2",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.1",
    "date-fns": "^3.0.6",
    "winston": "^3.11.0",
    "helmet": "^7.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/uuid": "^9.0.7",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "^1.1.0",
    "@vitest/ui": "^1.1.0",
    "ioredis-mock": "^8.9.0",
    "prisma": "^5.8.0",
    "supertest": "^6.3.3",
    "tsx": "^4.7.0",
    "vitest": "^1.1.0",
    "typescript": "^5.3.3"
  }
}
